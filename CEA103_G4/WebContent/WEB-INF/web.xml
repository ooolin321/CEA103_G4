<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	metadata-complete="true" version="3.0">
	<display-name>CEA103_G4</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<resource-ref>
		<description>DB Connection</description>
		<res-ref-name>jdbc/admin</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
				<!--localhotDB test 	 -->
	<resource-ref>
		<description>DB2 Connection</description>
		<res-ref-name>jdbc/CEA103_G4</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	
	<servlet>
		<servlet-name>ProductServlet</servlet-name>
		<servlet-class>com.product.model.ProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProductServlet</servlet-name>
		<url-pattern>/product/product.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LiveServlet</servlet-name>
		<servlet-class>com.live.controller.LiveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LiveServlet</servlet-name>
		<url-pattern>/live/live.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MsgServlet</servlet-name>
		<servlet-class>com.msg.controller.MsgServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MsgServlet</servlet-name>
		<url-pattern>/msg/msg.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PdcServlet</servlet-name>
		<servlet-class>com.pdc.controller.PdcServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PdcServlet</servlet-name>
		<url-pattern>/pdc/pdc.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Live_orderServlet</servlet-name>
		<servlet-class>com.live_order.controller.Live_orderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Live_orderServlet</servlet-name>
		<url-pattern>/live_order/live_order.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EmpServlet</servlet-name>
		<servlet-class>com.emp.controller.EmpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmpServlet</servlet-name>
		<url-pattern>/emp/emp.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FunServlet</servlet-name>
		<servlet-class>com.fun.controller.FunServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FunServlet</servlet-name>
		<url-pattern>/fun/fun.do</url-pattern>
	</servlet-mapping>
</web-app>
